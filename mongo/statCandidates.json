[
  {
    $match:
      /**
       * query: The query in MQL.
       */
      {
        UPDNVOL50: {
          $gt: 1.0
        },
        currentRatio: {
          $gt: 1.0
        },
        quickRatio: {
          $gt: 1.0
        },
        epsgrowth: {
          $gt: 0
        },
        grossProfitGrowth: {
          $gt: 0
        },
        netProfitMargin: {
          $gt: 0
        },
        netIncomeGrowth: {
          $gt: 0
        },
        returnOnEquity: {
          $gt: 0
        },
        revenueGrowth: {
          $gt: 0
        },
        operatingProfitMargin: {
          $gt: 0
        },
        operatingIncomeGrowth: {
          $gt: 0
        }
      }
  },
  {
    $group:
      /**
       * _id: The id of the group.
       * fieldN: The first field name.
       */
      {
        _id: "$tickerSymbol",
        count: {
          $sum: 1
        },
        updn: {
          $avg: "$UPDNVOL50"
        },
        updnmin: {
          $min: "$UPDNVOL50"
        },
        updnmax: {
          $max: "$UPDNVOL50"
        },
        dyvolv20a: {
          $avg: "$DYVOLV20A"
        },
        dyvolv50a: {
          $avg: "$DYVOLV50A"
        },
        dyvolv200a: {
          $avg: "$DYVOLV200A"
        },
        currRatio: {
          $avg: "$currentRatio"
        },
        quickRatio: {
          $avg: "$quickRatio"
        },
        roe: {
          $avg: "$returnOnEquity"
        },
        netProfitMargin: {
          $avg: "$netProfitMargin"
        },
        netIncomeGrowth: {
          $avg: "$netIncomeGrowth"
        },
        epsgrowth: {
          $avg: "$epsgrowth"
        },
        operatingProfitMargin: {
          $avg: "$operatingProfitMargin"
        },
        operatingIncomeGrowth: {
          $avg: "$operatingIncomeGrowth"
        },
        threeYRevenueGrowthPerShare: {
          $avg: "$threeYRevenueGrowthPerShare"
        },
        begin: {
          $min: "$priceDate"
        },
        end: {
          $max: "$priceDate"
        },
        weeklyopt: {
          $first: "$weeklyOptions"
        }
      }
  },
  {
    $match:
      /**
       * query: The query in MQL.
       */
      {
        count: {
          $gt: 1
        },
        end: {
          $gt: ISODate("2023-12-01")
        }
        //  weeklyopt: true,
      }
  },
  {
    $sort:
      /**
       * Provide any number of field/order pairs.
       */
      {
        count: -1
      }
  }
]